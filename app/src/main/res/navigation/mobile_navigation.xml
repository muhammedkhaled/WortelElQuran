<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.ertaqiwratel.presentation.activities.navUi.home.HomeFragment"
        android:label="@string/home_screen"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.ertaqiwratel.presentation.activities.navUi.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragmnet_profile" />
    <fragment
        android:id="@+id/nav_extra"
        android:name="com.example.ertaqiwratel.presentation.activities.navUi.extra.ExtraSessionFragment"
        android:label="@string/extra_session"
        tools:layout="@layout/fragment_extra_session" />
    <fragment
        android:id="@+id/nav_admin"
        android:name="com.example.ertaqiwratel.presentation.activities.navUi.admin.AdminChatFragment"
        android:label="@string/administration"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/myChiehkFrament"
        android:name="com.example.ertaqiwratel.presentation.activities.MyChiehkFrament"
        tools:layout="@layout/fragment_my_chiehk">
        <action
            android:id="@+id/action_myChiehkFrament_to_myChiekhInfo"
            app:destination="@id/myChiekhInfo"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/myChiekhInfo"
        android:name="com.example.ertaqiwratel.presentation.activities.MyChiekhInfo"
        tools:layout="@layout/fragment_my_chiekh_info" />
</navigation>